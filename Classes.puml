@startuml Classes


class Person {
    int : Money
    CarClass[] : Cars_Owned
}

class Dealer {
    string : Type
    CarClass[] : Cars
}

class Car {
    string : Brand
    int : Gear
    bool : On_State
}

class Engine {
    int : Horsepower
    int : Torque
}



Engine -left-> Car
Dealer -up-> Car

Car -up-> Person

@enduml